    "passport": "^0.4.0",
    "pug": "^2.0.0",
    "sequelize": "^4.35.2",
    "sequelize-auto-migrations": "^1.0.2",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.18.2",
    "eslint-config-google": "^0.9.1",
